{
  "name": "elementary",
  "version": "0.0.1",
  "author": "Eddort",
  "description": "Library for build atomic react-components",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "clean": "rm -rf build/*",
    "build:cjs": "NODE_ENV=cjs babel src -o build/index.cjs.js --ignore '**/*.test.js'",
    "build:esm": "NODE_ENV=esm babel src -o build/index.esm.js --ignore '**/*.test.js'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/quarkly/elementary.git"
  },
  "keywords": [
    "react",
    "styled-components",
    "emotion",
    "styled-system"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "ava": "^1.3.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "react": "^16.8.5",
    "react-test-renderer": "^16.8.5",
    "styled-components": "^4.2.0",
    "babel-jest": "^24.3.1",
    "jest": "^24.3.1"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/quarkly/elementary/issues"
  },
  "homepage": "https://gitlab.com/quarkly/elementary#readme",
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
